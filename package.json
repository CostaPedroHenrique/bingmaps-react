{
  "name": "bingmaps-react",
  "description": "An easy to use Bing Maps component for React apps",
  "author": {
    "name": "Miles Pratt",
    "email": "miles@thoughtpowder.com",
    "url": "https://milespratt.com"
  },
  "keywords": [
    "bing",
    "maps",
    "bingmaps",
    "bing maps",
    "react bing maps",
    "microsoft bingmaps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/milespratt/bingmaps-react"
  },
  "version": "0.3.7",
  "private": false,
  "module": "dist/BingMapsReact.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/src/index.js",
      "!**/src/serviceWorker.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCoverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src/components -d dist --copy-files && npm publish"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "react-test-renderer": "^16.12.0"
  }
}
